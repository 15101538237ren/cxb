.hide{display:none}

/* Header */

#header {	
	height:60px;
	text-align:right;
	background: url(../images/img01.jpg) repeat left top;
}


/*************************
		header
*************************/
.rheader{
	float:right;
	height:65px; 
	padding:0px 0px 0px 0px;
	/*padding:0px 15px 0px 20px;*/	
}

.rheaderBody {
	margin: 0;
	padding: 0;
	background: #F7F7F7 url(../images/img01.jpg) repeat left top;
	font-family: Georgia, "Times New Roman", Times, serif;
	font-size: 14px;
	color: #5A554E;
}

/*************************
		topMenu
*************************/
#topMenu{height:30px; }

#topMenuRight{
	/*width: 1000px;*/
	height: 65px;
	margin: 0 auto;
	padding: 0;
	text-align:right;
	/*background: url(../images/img04.jpg) no-repeat left top;*/
}

#topMenuRight ul{
	float: right;
	margin: 0;
	padding: 0px 25px 0px 0px;
	list-style: none;
	line-height: normal;
}

#topMenuRight li {
	float: right;
	display: block;
	width: 120px;
	height: 39px;
	margin-left: 10px;
	padding-top: 15px;
	background: url(../images/img05.jpg) repeat-x left top;
	font-family: Georgia, "Times New Roman", Times, serif;
	text-decoration: none;
	text-align: center;
	font-size: 18px;
	font-weight: normal;
	color: #FFFFFF;
	border: none;
}
#topMenuRight li a{
	color:#FFFFFF;
	border: none;
	text-decoration:none;
}

#topMenuRight li:hover, #topMenuRight li.selected {
	background: url(../images/img06.jpg) no-repeat left top;
	padding-left: 0;
	color: #493E2B;
}

#topMenuRight li:hover a,#topMenuRight li.selected a{
	text-decoration:none; 
	color:#493E2B;
}


/*************************
		leftMenu
*************************/
.leftMenu {
	width:100%; 
	margin:0px auto;	
	font-family: 微软雅黑, 宋体;
}

.leftMenu .leftMenuFrame {	
	padding-top:2px;
	background: url(../images/leftPaneBg.gif) no-repeat right top;
}

.leftMenu .leftMenuTop {	
	background: #f3f3f6 url(../images/leftMenuTop.jpg) no-repeat right top;
	
}

.leftMenu .leftMenuBtm {	
	height:2px;
	background: #f3f3f6 url(../images/leftMenuBtm.jpg) no-repeat right top;
	
}

.leftMenu .module {
	padding-top:2px;
	text-align:center;
	background: #f3f3f6 url(../images/test.jpg) repeat-y;
}

.module > h4 {
	font-size:20px;	
}

div.singleMenu{
	background: #f3f3f6 url(../images/test.jpg) repeat-y;
}

.singleMenu a:link, .singleMenu a:visited, .singleMenu a:hover{
	text-decoration:none;
	display:block; 
	height:28px; 
	line-height:28px; 
	font-size:16px; 
	color:#336699; 
	text-align:center;	
}
.singleMenu a.selected{font-weight:bold }
.singleMenu > h5 {
	font-size:20px;
}
/*************************
		audit
*************************/
.auditSubTitle{position:relative; margin:15px auto 2px auto;border-bottom:#336699 2px solid; height:25px; line-height:25px; background:#e8e8e8 url(../images/info.gif) left 3px no-repeat; padding-left:20px}
.auditSubTitle #objInfoShow{position:absolute; right:5px; top:5px; width:16px; height:16px; cursor:pointer}
.auditSubTitle #flowInfoShow{position:absolute; right:5px; top:5px; width:16px; height:16px; cursor:pointer}
.shrink{background:transparent url(../images/arrow_down2.gif) left top no-repeat; width:16px; height:16px;}
.expand{background:transparent url(../images/arrow_top2.gif) left top no-repeat; width:16px; height:16px;}

